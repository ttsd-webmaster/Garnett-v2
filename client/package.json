{
  "name": "garnett",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "0.28.7",
    "dotenv": "^4.0.0",
    "eslint": "4.18.2",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "flow-bin": "^0.95.1",
    "fs-extra": "3.0.1",
    "html-critical-webpack-plugin": "^1.1.0",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react-dev-utils": "^4.2.1",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3",
    "workbox-sw": "^3.1.0",
    "workbox-webpack-plugin": "^3.1.0"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "chart.js": "^2.7.2",
    "material-ui": "^0.20.1",
    "material-ui-fullscreen-dialog": "^0.7.2",
    "mobile-pull-to-refresh": "^0.2.1",
    "npm": "^6.9.0",
    "react": "^16.3.1",
    "react-chartjs-2": "^2.7.3",
    "react-countup": "^3.0.3",
    "react-day-picker": "^7.3.0",
    "react-dom": "^16.3.1",
    "react-infinite-scroller": "^1.2.4",
    "react-lazyload": "^2.3.0",
    "react-loadable": "^5.3.1",
    "react-portal": "^4.1.5",
    "react-router-dom": "^5.1.2",
    "react-sweet-progress": "^1.1.2",
    "react-swipeable-views": "^0.13.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "flow": "node_modules/.bin/flow check"
  },
  "proxy": {
    "api": {
      "target": "http://localhost:4000/"
    }
  },
  "main": "./src/index.js",
  "repository": "https://github.com/UCSDTT/Garnett-v2.git",
  "author": "Blake Desktop <bhhuynh@ucsd.edu>",
  "license": "MIT",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
